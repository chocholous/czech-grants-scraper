{
    "title": "MZD Grant Output",
    "type": "object",
    "schemaVersion": 1,
    "properties": {
        "startUrls": {
            "title": "Start URLs",
            "type": "array",
            "description": "URLs to start scraping from",
            "editor": "requestListSources",
            "default": [{"url": "https://mzd.gov.cz/category/dotace-a-programove-financovani/narodni-dotacni-programy-2026/"}]
        },
        "proxyConfiguration": {
            "title": "Proxy Configuration",
            "type": "object",
            "description": "Proxy settings",
            "editor": "proxy",
            "default": { "useApifyProxy": false }
        },
        "mode": {
            "title": "Mode",
            "type": "string",
            "description": "Scrape mode",
            "default": "scrape",
            "enum": ["scrape", "test"]
        }
    },
    "required": ["startUrls"]
}